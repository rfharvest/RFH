<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Rotary First Harvest</title>
    <link href="@Url.Content("~/Content/NewSite.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/NewHeaderFooter.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <link rel="icon" type="image/ico" href="/favicon.ico">
</head>
<body>
    @Html.Partial("NewUi/_Header")

    <div class="midsection">
        <div class="navPaneSection">
            <div class="categoryNavPane sidePane">
                @Html.Action("ListCategory", "NavigationList")
            </div>
            <div class="hostSiteNavPane sidePane">
                @Html.ActionLink("General Resources", "GeneralResource", "Home")
            </div>
            <div class="hostSiteNavPane sidePane">
                @Html.Action("ListSite", "NavigationList")
            </div>
        </div>

        <div class="contentSection">
            @RenderBody()
        </div>

        <div class="clear"></div>
    </div>

    @Html.Partial("NewUi/_Footer")
</body>
</html>
