@model IEnumerable<RFH.Models.SuperCategoryNavigationList>

@foreach (var superCategory in Model) {
  <li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">@superCategory.Name.ToLowerInvariant()<b class="caret"></b></a>
      <ul class="dropdown-menu">
          @foreach (var category in superCategory.Categories)
          {
              <li><a href="@category.Url">@category.Name</a></li>
          }
      </ul>
  </li>
}