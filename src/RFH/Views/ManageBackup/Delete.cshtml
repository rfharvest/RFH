@model string

<h2>@Html.Raw(@PageData["SectionTitle"]) Delete</h2>

<div class="formContainer">

    <h3>Are you sure you want to delete this backup file?</h3>

    <div class="display-label">Backup File</div>
    <div class="display-field">
        @Model
    </div>

    @using (Html.BeginForm()) {
        <p>
            <input type="submit" value="Delete Backup" class="standardButton" />
        </p>
    }

</div>

<p>
    @Html.ActionLink("Back to List", "Index")
</p>